<!--pages/home/home.wxml-->
<view bindtouchstart="touchStart" bindtouchmove="touchMove" bindtouchend="touchEnd" style="transform: translateY({{translateY}}px)" class="wrap"
  hover-class="" hover-stop-propagation="false">
  <view wx:for="{{10}}" wx:for-item="i" wx:key="i" class="item" hover-class="none" hover-stop-propagation="false">
    {{i}}
  </view>
</view>